<div class="col-md-3 technology-right">
	<div class="blo-top1">
		<div class="tech-btm">
			<h4>Top Ten Singles </h4>
			<% @musics.highest_voted.each do |m| %>
			<div class="blog-grids">
				<div class="blog-grid-left">
					<%= link_to m do %>
						<%= image_tag m.albumimage.url, class: "img-responsive" %>
						<% end %>
				</div>
				<div class="blog-grid-right">
					<h5> <%=link_to m.songname %> </h5>
				</div>
				<br>
			<% end %>
			<div class="clearfix"></div>
			<div class="clearfix"></div>
			<div class="clearfix"></div><div class="clearfix"></div>
			<br /><br>
				<h4>Top Videos</h4>

					<% @videos.highest_voted.each do |v| %>
					<div class="blog-grids">
					<div class="blog-grids-left">
						<%= link_to v, target: '_blank' do %>
						<%= image_tag("https://img.youtube.com/vi/#{v.uid}/mqdefault.jpg", alt: v.title,
						class: ' img-responsive ') %>

						<%end%>
					</div>
					<br />
					<div class="blog-grid-right">
						<h5><%= link_to v.title %><h5>
					</div>
						</div>
						<% end%>


						<div class="clearfix"></div><div class="clearfix"></div>
						<br />
						<h4> Chapo Ba </h4>
						<% @facts.each do |f|%>
						<div class="blog-grids">
							<div class="blog-grids-left">
								<%= link_to f do %>
									<%= image_tag f.thumbnail.url, class: "img-responsive"%>
									<% end %>
								</div>
								<br>
								<div class="blog-grid-right">
									<h5><%= f.fun1 %> </h5>
									<br>
									<h5><%= f.fun2 %> </h5>
									<br>
									<h5><%= f.fun3 %> </h5>
								</div>
							</div>
							<% end %>






		</div>
	</div>


</div>
<div class="clearfix"></div>
<!-- technology-right -->
